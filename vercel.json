{
    "rewrites": [
        {
            "source": "/a11y/(.*)",
            "destination": "/a11y"
        },
        {
            "source": "/a11y-lightning/(.*)",
            "destination": "/a11y-lightning"
        },
        {
            "source": "/source",
            "destination": "/a11y-lightning"
        }
    ],
    "buildCommand": "npm run build -- --force",
    "outputDirectory": "../dist"
}